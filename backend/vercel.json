{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 30,
      "memory": 1024,
      "includeFiles": "node_modules/**"
    }
  },
  "installCommand": "npm install --production=false",
  "buildCommand": "cd ../frontend && npm install && npm run build"
}
